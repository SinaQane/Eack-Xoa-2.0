      fx:controller="apps.mainpage.pages.profile.view.ProfilePaneFXML">
    <ImageView fx:id="profilePicture" fitHeight="120.0" fitWidth="120.0" layoutX="36.0" layoutY="37.0"
               pickOnBounds="true" preserveRatio="true"/>
    <Text fx:id="nameText" fill="WHITE" layoutX="170.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Name" wrappingWidth="630.0">
        <font>
            <Font size="22.0"/>
        </font>
    </Text>
    <Text fx:id="usernameText" fill="WHITE" layoutX="171.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="\@username" wrappingWidth="630.0">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <Text fx:id="emailText" fill="WHITE" layoutX="171.0" layoutY="113.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Email: N/A" wrappingWidth="630.0">
        <font>
            <Font size="16.0"/>
        </font>
    </Text>
    <Text fx:id="birthdateText" fill="WHITE" layoutX="171.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Birthdate: N/A" wrappingWidth="630.0">
        <font>
            <Font size="16.0"/>
        </font>
    </Text>
    <Text fx:id="phoneNumberText" fill="WHITE" layoutX="171.0" layoutY="153.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Phone Number: N/A" wrappingWidth="630.0">
